#Create a new bucket to store the Elastic Beanstalk Templates
aws cloudformation create-stack --stack-name S3BucketTemplates --template-file file://s3.yml

#Build Everything with one template
aws cloudformation create-stack --stack-name tt-full-flight-01 --template-url https://s3-tt-templates.s3-ap-southeast-2.amazonaws.com/main.yml

#Build the bucket to house the templates
aws cloudformation create-stack --stack-name ttk-s3 --template-url https://s3-tt.s3-ap-southeast-2.amazonaws.com/s3.yml

#Build the VPC
aws cloudformation create-stack --stack-name tt-vpc --template-url https://s3-tt.s3-ap-southeast-2.amazonaws.com/vpc.yml
